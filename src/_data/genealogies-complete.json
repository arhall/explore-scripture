  "comparisons": {
    "messianic": {
      "id": "messianic",
      "title": "Messianic Genealogy: Matthew 1 vs Luke 3",
      "description": "Compare the royal and prophetic presentations of Jesus' lineage.",
      "left": {
        "label": "Matthew's Royal Line",
        "genealogy": "matthew_genealogy",
        "focus": "Highlights Jesus as the promised Son of David and legal heir to the throne."
      },
      "right": {
        "label": "Luke's Universal Line",
        "genealogy": "luke_genealogy",
        "focus": "Traces Jesus back to Adam to show him as Son of Man for all peoples."
      },
      "notes": [
        {
          "type": "difference",
          "heading": "Royal vs. Prophetic Branch",
          "body": "Matthew follows Solomon and the kings of Judah; Luke follows Nathan, another son of David."
        },
        {
          "type": "convergence",
          "heading": "Shared Exilic Convergence",
          "body": "Both lines meet again with Shealtiel and Zerubbabel, culminating in Joseph and Jesus."
        },
